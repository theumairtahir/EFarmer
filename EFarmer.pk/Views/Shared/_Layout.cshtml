@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer _localizer
@{
    string rtl = "";
    if (!_localizer["rtl"].IsResourceNotFound)
    {
        rtl = _localizer["rtl"].Value;
    }
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <!--[if IE]>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <![endif]-->
    <meta name="description" content="">
    <meta name="author" content="ScriptsBundle">
    <title>@ViewData["Title"] | @EFarmer.pk.Common.CommonValues.APP_NAME</title>
    <!-- =-=-=-=-=-=-= Favicons Icon =-=-=-=-=-=-= -->
    <link rel="icon" href="@Url.Content("~/images/favicon.ico")" type="image/x-icon" />
    <!-- =-=-=-=-=-=-= Mobile Specific =-=-=-=-=-=-= -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <!-- =-=-=-=-=-=-= Bootstrap CSS Style =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="@Url.Content("~/css/bootstrap.css")" type="text/css">
    <!-- =-=-=-=-=-=-= Template CSS Style =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="@Url.Content("~/css/style.css")" type="text/css">
    <!-- =-=-=-=-=-=-= Font Awesome =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="@Url.Content("~/css/font-awesome.css")" type="text/css">
    <!-- =-=-=-=-=-=-= Flat Icon =-=-=-=-=-=-= -->
    <link href="@Url.Content("~/css/flaticon.css")" rel="stylesheet" type="text/css">
    <!-- =-=-=-=-=-=-= Et Line Fonts =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="@Url.Content("~/css/et-line-fonts.css")" type="text/css">
    <!-- =-=-=-=-=-=-= Menu Drop Down =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="@Url.Content("~/css/forest-menu.css")" type="text/css">
    <!-- =-=-=-=-=-=-= Animation =-=-=-=-=-=-= -->
    <link rel="stylesheet" href="@Url.Content("~/css/animate.min.css")" type="text/css">
    <!-- =-=-=-=-=-=-= Select Options =-=-=-=-=-=-= -->
    <link href="@Url.Content("~/css/select2.min.css")" rel="stylesheet" type="text/css" />
    <!-- =-=-=-=-=-=-= noUiSlider =-=-=-=-=-=-= -->
    <link href="@Url.Content("~/css/nouislider.min.css")" rel="stylesheet" type="text/css" />
    <!-- =-=-=-=-=-=-= Listing Slider =-=-=-=-=-=-= -->
    <link href="@Url.Content("~/css/slider.css")" rel="stylesheet" type="text/css" />
    @if (!_localizer["rtl"].IsResourceNotFound)
    {
        <!--Bootstrap RTL-->
        <link href="@Url.Content("~/css/bootstrap-rtl.css")" rel="stylesheet">
    }
    <!-- =-=-=-=-=-=-= Owl carousel =-=-=-=-=-=-= -->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/css/owl.carousel.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/css/owl.theme.css")">
    <!-- =-=-=-=-=-=-= Check boxes =-=-=-=-=-=-= -->
    <link href="@Url.Content("~/skins/minimal/minimal.css")" rel="stylesheet" type="text/css" />
    <!-- =-=-=-=-=-=-= Responsive Media =-=-=-=-=-=-= -->
    <link href="@Url.Content("~/css/responsive-media.css")" rel="stylesheet" type="text/css" />
    <!-- =-=-=-=-=-=-= Template Color =-=-=-=-=-=-= -->
    <link rel="stylesheet" id="color" href="@Url.Content("~/css/colors/defualt.css")" type="text/css" />
    <!-- =-=-=-=-=-=-= For Style Switcher =-=-=-=-=-=-= -->
    <link rel="stylesheet" id="theme-color" type="text/css" href="@Url.Content("~/css/colors/defualt.css")" />
    <!-- JavaScripts -->
    <script src="@Url.Content("~/js/modernizr.js")"></script>
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    @*[if lt IE 9]>*@
    <environment exclude="Development">
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    </environment>
    @RenderSection("Styles", required: false)
    @*<![endif]*@
</head>
<body class="@rtl">
    <div class="banner">
        @RenderSection("PageBanner", false)
    </div>
    <!-- =-=-=-=-=-=-= Preloader =-=-=-=-=-=-= -->
    <div id="loader-wrapper">
        <div id="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
    <!-- =-=-=-=-=-=-= Color Switcher =-=-=-=-=-=-= -->
    @*<div class="color-switcher" id="choose_color">
            <a href="#." class="picker_close"><i class="fa fa-gear"></i></a>
            <h5>STYLE SWITCHER</h5>
            <div class="theme-colours">
                <p> Choose Colour style </p>
                <ul>
                    <li>
                        <a href="#." class="defualt" id="defualt"></a>
                    </li>
                    <li>
                        <a href="#." class="green" id="green"></a>
                    </li>
                    <li>
                        <a href="#." class="blue" id="blue"></a>
                    </li>
                    <li>
                        <a href="#." class="red" id="red"></a>
                    </li>

                    <li>
                        <a href="#." class="sea-green" id="sea-green"></a>
                    </li>

                </ul>
            </div>
            <div class="clearfix"> </div>
        </div>*@
    <!-- =-=-=-=-=-=-= Light Header =-=-=-=-=-=-= -->
    <div class="colored-header">
        <!-- Top Bar -->
        <partial name="_HeaderTop" />
        <!-- Top Bar End -->
        <!-- Navigation Menu -->
        <nav id="menu-1" class="mega-menu">
            <!-- menu list items container -->
            <section class="menu-list-items">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 col-md-12">
                            <!-- menu logo -->
                            <ul class="menu-logo">
                                <li>
                                    <a href="@Url.Action("Index", "Home")"><img src="@Url.Content("~/images/logo.png")" alt="logo"> </a>
                                </li>
                            </ul>
                            <!-- menu links -->
                            <partial name="_NavBar" />
                        </div>
                    </div>
                </div>
            </section>
        </nav>
    </div>
    <!-- Navigation Menu End -->
    <!-- =-=-=-=-=-=-= Light Header End  =-=-=-=-=-=-= -->
    <!-- =-=-=-=-=-=-= Transparent Breadcrumb =-=-=-=-=-=-= -->
    <div class="page-header-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="header-page">
                        <h1>@ViewData["Title"]</h1>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Small Breadcrumb -->
    @RenderSection("Breadcrumb", false)

    <!-- Small Breadcrumb -->
    <!-- =-=-=-=-=-=-= Transparent Breadcrumb End =-=-=-=-=-=-= -->
    <!-- =-=-=-=-=-=-= Main Content Area =-=-=-=-=-=-= -->
    <div class="main-content-area clearfix">
        <!-- =-=-=-=-=-=-= Latest Ads =-=-=-=-=-=-= -->
        @RenderBody()
        <!-- =-=-=-=-=-=-= Ads Archives End =-=-=-=-=-=-= -->
        <!-- =-=-=-=-=-=-= FOOTER =-=-=-=-=-=-= -->
        <!-- =-=-=-=-=-=-= FOOTER END =-=-=-=-=-=-= -->
        <partial name="_Footer" />
    </div>
    <!-- Main Content Area End -->
    @RenderSection("ModalSection", false)
    <!-- Post Ad Sticky -->
    <partial name="_stickyButton" />
    <!-- Back To Top -->
    <a href="#0" class="cd-top">Top</a>
    <!-- =-=-=-=-=-=-= JQUERY =-=-=-=-=-=-= -->
    <script src="@Url.Content("~/js/jquery.min.js")"></script>
    <!-- Bootstrap Core Css  -->
    <script src="@Url.Content("~/js/bootstrap.min.js")"></script>
    <!-- Jquery Easing -->
    <script src="@Url.Content("~/js/easing.js")"></script>
    <!-- Menu Hover  -->
    <script src="@Url.Content("~/js/forest-megamenu.js")"></script>
    <!-- Jquery Appear Plugin -->
    <script src="@Url.Content("~/js/jquery.appear.min.js")"></script>
    <!-- Numbers Animation   -->
    <script src="@Url.Content("~/js/jquery.countTo.js")"></script>
    <!-- Jquery Smooth Scroll  -->
    <script src="@Url.Content("~/js/jquery.smoothscroll.js")"></script>
    <!-- Jquery Select Options  -->
    <script src="@Url.Content("~/js/select2.min.js")"></script>
    <!-- noUiSlider -->
    <script src="@Url.Content("~/js/nouislider.all.min.js")"></script>
    <!-- Carousel Slider  -->
    <script src="@Url.Content("~/js/carousel.min.js")"></script>
    <script src="@Url.Content("~/js/slide.js")"></script>
    <!-- Image Loaded  -->
    <script src="@Url.Content("~/js/imagesloaded.js")"></script>
    <script src="@Url.Content("~/js/isotope.min.js")"></script>
    <!-- CheckBoxes  -->
    <script src="@Url.Content("~/js/icheck.min.js")"></script>
    <!-- Jquery Migration  -->
    <script src="@Url.Content("~/js/jquery-migrate.min.js")"></script>
    <!-- Sticky Bar  -->
    <script src="@Url.Content("~/js/theia-sticky-sidebar.js")"></script>
    <!-- Style Switcher -->
    <script src="@Url.Content("~/js/color-switcher.js")"></script>
    <!-- Template Core JS -->
    <script src="@Url.Content("~/js/custom.js")"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
  <ItemGroup>
    <Reference Include="EntityGrabber">
      <HintPath>..\Packages\EntityGrabber\EntityGrabber.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Update="App_Data\w3layouts-License.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
